/*
 * PERSON INFORMATION
 *  <div class="person-info-container">
 *    <div class="person-gender-icon fs-icon-*"></div>
 *    <ul class="person-info">
 *      <li class="person-name">John Adam Doe</li>
 *      <li class="person-lifeSpan">1830-1900</li>
 *      <li class="person-id">SKJ-DIY</li>
 *    </ul>
 *  </div>
 *
 * COUPLE RELATIONSHIPS
 *  <div class="person-info-container couple-husband">
 *    <div class="person-gender-icon fs-icon-*"></div>
 *    <ul class="person-info">
 *     <li class="person-name">John Adam Doe</li>
 *      <li class="person-lifeSpan">1830-1900</li>
 *      <li class="person-id">SKJ-DIY</li>
 *    </ul>
 *  </div>
 *  <div class="person-info-container couple-wife">
 *    <div class="person-gender-icon fs-icon-*"></div>
 *    <ul class="person-info">
 *      <li class="person-name">John Adam Doe</li>
 *      <li class="person-lifeSpan">1830-1900</li>
 *      <li class="person-id">SKJ-DIY</li>
 *    </ul>
 *  </div>
 *
 * PEDIGREE RELATIONSHIPS
 *  <div class="person-info-container">
 *    <div class="person-gender-icon fs-icon-*"></div>
 *    <ul class="person-info">
 *      <li class="person-name">John Adam Doe</li>
 *      <li class="person-lifeSpan">1830-1900</li>
 *      <li class="person-id">SKJ-DIY</li>
 *    </ul>
 *  </div>
 *  <div class="person-info-container">
 *    <div class="person-gender-icon fs-icon-*"></div>
 *    <ul class="person-info">
 *      <li class="person-name add-spouse">Add Spouse</li>
 *    </ul>
 *  </div>
 */
.person-info-container {

  .person-gender-icon {

    &[class*=fs-icon-] {
      display: inline-block;
      vertical-align: middle;
      margin-right: 6px;
      position: relative;
    }

    &[class*=fs-icon-small] {
      vertical-align: top;
      margin-top: 7px;

      + .person-info {
        width: 90%;
      }
    }

    &[class*=fs-icon-medium] + .person-info {
      width: 90%
    }

    &[class*=fs-icon-large] + .person-info {
      width: 75%;
    }
  }

  .person-info {
    padding: 0;
    margin: 0;
    list-style: none;
    display: inline-block;
    vertical-align: middle;

    clearfix();
  }

  .person-name {
    overflow: ellipsis;
    font-size: $medium-size;
    line-height: $medium-lineHieght;

    & > h1,& > h2,& > h3,& > h4 {
      padding: 0;
      margin: 0;
    }
  }
  .person-lifeSpan,
  .person-id, {
    float: left;
    font-size: $small-size;
    line-height: $small-lineHieght;
  }

  .person-lifeSpan:not(:empty) + .person-id:not(:empty):before {
    display: inline-block;
    vertical-align: middle;
    margin: 0 8px 3px 8px;
    content: '\2022';
  }

  // couple relationships
  &.couple-husband .person-gender-icon,
  &.couple-wife .person-gender-icon {
    @extend .person-info-container .person-gender-icon[class*=fs-icon-];

    &:before {
      content: '';
      width: 8px;
      height: 26px;
      border: 1px solid #000;
      border-right: 0;
      position: absolute;
      right: 100%;
    }
  }


  &.couple-husband .person-gender-icon:before {
    border-bottom: 0;
    top: 50%;
  }

  &.couple-wife {
    margin-top: 5px;

    .person-gender-icon:before {
      border-top: 0;
      bottom: 50%;
    }
  }

  // pedigree relationship
  .add-spouse {
    line-height: 40px;
  }
}